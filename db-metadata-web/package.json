    "qs": "^6.11.2",
    "screenfull": "^6.0.2",
    "vue": "^2.7.16",
    "vue-i18n": "^8.28.1",
    "vue-json-editor": "^1.4.3",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.6.5",
    "vue-tinymce-text": "0.1.2",
